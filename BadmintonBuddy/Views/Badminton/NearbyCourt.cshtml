@model List<BadmintonBuddy.Models.Club>
@if (Model.Count != 0)
{ 
    <p>
        <h3>
            Nearby Baminton Courts</h3>
    </p>

    foreach (var club in Model)
    {
    <div class="panel panel-default">
        <div class="panel-heading">
            <a href="/court/@club.ClubName/@club.ClubID">@club.ClubName</a></div>
        <div class="panel-body">
            <p>@club.Address</p>
            <div class="col-xs-offset-3 col-xs-9">
                <a href="/court/@club.ClubName/@club.ClubID">more information >></a></div>
        </div>
    </div>  
    }
}
